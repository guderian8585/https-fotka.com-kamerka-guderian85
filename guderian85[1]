        function show_cookie_msg() {
            document.getElementById('cookies_info').style.display = 'block';
            $('#cookie-close').click(function() {
                $('#cookies_info').hide();
                $('body').css('padding-bottom', '0');
                _setLocalCookie('cookie_accept',1,60*24*365);
                return false;
            });
        }
        $(function() {
            try {
                var cookie_accept = _getLocalCookie("cookie_accept");
                if(cookie_accept == null) {
                   show_cookie_msg();
                }
            } catch(e) {
                show_cookie_msg();
            }
        });
